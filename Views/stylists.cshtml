<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    <title>All Stylists</title>
  </head>
  <body>
    @if(@Model.Count == 0)
    {
      <p>No Stylists have been registered yet.</p>
    }
    else
    {
      <ul>
      @foreach(var stylist in Model)
      {
        <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</a></li>
      }
    </ul>
    }
    <a href="/stylists/new">Add a new stylist.</a>
  </body>
</html>
